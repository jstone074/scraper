<div class="filler"></div>

<div class="articles container">

    {{#each articles}}
<div class="card">
  <div class="card-header panel-title">
    <h5>{{title}}
    <button data-id={{_id}} class="btn btn-danger pull-right save-article">Save Article</button>
    </h5>
  </div>
  <div class="card-body">
    <p class="card-text summary">{{summary}}</p>
    <a href="#" class="artLink" href={{link}}>{{link}}</a>
          
    </div>
     <br />
    <br />
    {{/each}}

</div>